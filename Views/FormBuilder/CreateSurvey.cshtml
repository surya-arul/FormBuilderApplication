@model FormBuilderMVC.DTOs.Base.SurveysDto

@{
    ViewData["Title"] = "Create Survey";
}

<h4>Create Survey</h4>
<hr />
<div class="container body-content border p-3 mt-4 mb-4">
    <form asp-action="AddSurvey" method="post">
